{
  "name": "sveltekit-fullstack-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "format:routes": "prettier --write ./src/lib/ROUTES.ts",
    "prisma:gen": "prisma format; prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:init": "prisma migrate dev --name init",
    "db:deploy": "prisma migrate deploy",
    "db:sync": "prisma db push",
    "db:resolve": "prisma migrate resolve",
    "db:reset": "prisma migrate reset"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^5.0.0",
    "@neoconfetti/svelte": "^2.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sveltejs/adapter-auto": "^3.2.4",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/typography": "^0.5.14",
    "@types/eslint": "^9.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "lucide-svelte": "^0.437.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-headless-table": "^0.18.2",
    "svelte-motion": "^0.12.2",
    "sveltekit-superforms": "^2.17.0",
    "sweetalert2": "^11.12.4",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.3",
    "vite-plugin-kit-routes": "^0.6.10",
    "zod": "^3.23.8"
  },
  "type": "module",
  "dependencies": {
    "@internationalized/date": "^3.5.5",
    "bits-ui": "^0.21.13",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.18",
    "prisma": "^5.19.0",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1"
  }
}